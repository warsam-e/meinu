{
	"name": "meinu",
	"description": "Simplifies the creation and handling of slash commands in Discord bots.",
	"author": "S0n1c",
	"version": "7.2.3",
	"license": "GPL-3.0-only",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "bun --watch ./lib/tests/index.ts",
		"sharding": "bun --watch ./lib/tests/sharding.ts",
		"build:lib": "bun run build.ts",
		"build:docs": "typedoc --options typedoc.json",
		"build": "bun build:lib && bun build:docs"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@types/bun": "1.2.19",
		"typedoc-github-theme": "0.3.0",
		"typescript": "5.8.3"
	},
	"dependencies": {
		"chalk": "5.4.1",
		"discord.js": "14.21.0",
		"dotenv": "17.2.1"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/itss0n1c/meinu.git"
	},
	"bugs": {
		"url": "https://github.com/itss0n1c/meinu/issues"
	},
	"homepage": "https://s0n1c.ca/meinu"
}
